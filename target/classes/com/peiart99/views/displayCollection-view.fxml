<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.peiart99.controllers.DisplayCollectionViewController">
   <children>
      <TableView fx:id="collectionTableView" layoutX="14.0" layoutY="14.0" prefHeight="203.0" prefWidth="266.0">
        <columns>
          <TableColumn fx:id="collectionTitleColumn" prefWidth="265.0" text="Title" />
        </columns>
      </TableView>
      <TableView fx:id="seriesTableView" layoutX="14.0" layoutY="216.0" prefHeight="170.0" prefWidth="266.0">
         <columns>
            <TableColumn fx:id="seriesTitleColumn" prefWidth="194.0" text="Title" />
            <TableColumn fx:id="seriesVolumeColumn" minWidth="0.0" prefWidth="71.0" text="Volume" />
         </columns>
      </TableView>
      <Button layoutX="300.0" layoutY="14.0" mnemonicParsing="false" onAction="#switchToAddBook" prefHeight="58.0" prefWidth="94.0" text="Add Book" />
      <Button layoutX="407.0" layoutY="14.0" mnemonicParsing="false" prefHeight="58.0" prefWidth="94.0" text="Add Series" />
      <GridPane layoutX="300.0" layoutY="120.0" prefHeight="160.0" prefWidth="279.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="95.0" minWidth="10.0" prefWidth="56.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="219.0" minWidth="10.0" prefWidth="205.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Title:" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Author:" GridPane.rowIndex="1" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Publisher:" GridPane.rowIndex="2" />
            <Text fx:id="volumeLabel" strokeType="OUTSIDE" strokeWidth="0.0" text="Volume:" GridPane.rowIndex="3" />
            <Text fx:id="uniqueLabel1" strokeType="OUTSIDE" strokeWidth="0.0" GridPane.rowIndex="4" />
            <Text fx:id="uniqueLabel2" strokeType="OUTSIDE" strokeWidth="0.0" GridPane.rowIndex="5" />
            <Text fx:id="titleText" strokeType="OUTSIDE" strokeWidth="0.0" text="N/A" GridPane.columnIndex="1" />
            <Text fx:id="authorText" strokeType="OUTSIDE" strokeWidth="0.0" text="N/A" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Text fx:id="publisherText" strokeType="OUTSIDE" strokeWidth="0.0" text="N/A" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Text fx:id="volumeText" strokeType="OUTSIDE" strokeWidth="0.0" text="N/A" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Text fx:id="uniqueText1" strokeType="OUTSIDE" strokeWidth="0.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <Text fx:id="uniqueText2" strokeType="OUTSIDE" strokeWidth="0.0" GridPane.columnIndex="1" GridPane.rowIndex="5" />
         </children>
      </GridPane>
   </children>
